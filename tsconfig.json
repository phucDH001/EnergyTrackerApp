{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/tabs/Profile.tsx",
    "app/tabs/index.tsx",
    "app/tabs/AddNew.tsx",
    "app/tabs/_layout.tsx"
  ]
}
